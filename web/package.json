{
  "name": "aot-vue-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:openshift": "vue-cli-service build --dest ../openshift/templates/nginx/dist ",
    "build:openshift:dev": "vue-cli-service build --mode dev --dest ../openshift/templates/nginx/dist && npm run build:add-artifacts && git commit -m \"Automated build: dev\"",
    "build:openshift:test": "vue-cli-service build --mode testing --dest ../openshift/templates/nginx/dist && npm run build:add-artifacts && git commit -m \"Automated build: test\"",
    "build:openshift:prod": "vue-cli-service build --mode production --dest ../openshift/templates/nginx/dist && npm run build:add-artifacts && git commit -m \"Automated build: prod\"",
    "build:add-artifacts": "git add \"../openshift/templates/nginx/dist\"",
    "build:dev": "vue-cli-service build --mode development",
    "dev": "node build/dev-server.js",
    "lint": "vue-cli-service lint --no-fix",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-s ervice test:unit",
    "unit": "jest --config tests/unit/jest.conf.js --silent"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.2.0",
    "axios": "^0.21.1",
    "axios-cache-adapter": "^2.3.0",
    "echarts": "^4.1.0",
    "font-awesome": "^4.7.0",
    "i": "^0.3.6",
    "jspdf": "^1.5.3",
    "jspdf-autotable": "^3.4.3",
    "keycloak-js": "^20.0.0",
    "localforage": "^1.7.3",
    "localforage-memoryStorageDriver": "^0.9.2",
    "material-design-icons-iconfont": "^4.0.5",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.24.0",
    "node-gyp": "^6.1.0",
    "object-assign": "^4.1.1",
    "object-merge": "^2.5.1",
    "path": "^0.12.7",
    "path-to-regexp": "^3.0.0",
    "quill": "^1.3.6",
    "register-service-worker": "^1.6.2",
    "roboto-fontface": "*",
    "url-parse": "^1.4.4",
    "v-calendar": "^1.0.4",
    "vee-validate": "^2.1.4",
    "vue": "2.6.8",
    "vue-currency-input": "^1.17.2",
    "vue-fullcalendar": "^1.0.9",
    "vue-i18n": "^6.1.1",
    "vue-perfect-scrollbar": "^0.1.0",
    "vue-provider": "^1.1.0",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "3.0.2",
    "vue-timeago": "^3.3.1",
    "vuetify": "^1.5.5",
    "vuex": "^3.0.1",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/preset-flow": "latest",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-e2e-nightwatch": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-pwa": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "^3.5.0",
    "@vue/cli-service": "^3.5.1",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "axios-mock-adapter": "^1.16.0",
    "babel-eslint": "latest",
    "babel-jest": "23.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.3.2",
    "dev-server": "^0.1.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.95.1",
    "jest": "^22.0.4",
    "jest-serializer-vue": "^0.3.0",
    "material-design-icons-iconfont": "^4.0.5",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass": "^1.17.2",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-jest": "^1.0.2",
    "vue-template-compiler": "2.6.8",
    "vuetify-loader": "^1.0.5",
    "webpack": "^4.30.0"
  },
  "devtool": "source-map",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "linebreak-style": 0,
      "import/prefer-default-export": "off",
      "no-plusplus": "off",
      "prefer-promise-reject-errors": "off",
      "no-param-reassign": "off",
      "max-len": [
        "error",
        {
          "code": 200
        }
      ],
      "object-curly-newline": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 6.0.0"
  }
}
